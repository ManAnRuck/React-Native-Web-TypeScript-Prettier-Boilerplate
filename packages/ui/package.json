{
  "name": "@myproject/ui",
  "version": "1.0.0",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "concurrently \"yarn storybook\" \"yarn watch\"",
    "typecheck": "tsc --noEmit",
    "watch": "nodemon --exec yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "tslint --project tsconfig.json && yarn typecheck",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps"
  },
  "dependencies": {
    "formik": "1.5.7",
    "react": "16.8.2",
    "semantic-ui-calendar-react": "0.15.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.87.2",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "1.0.0-rc2",
    "@storybook/addon-a11y": "5.0.11",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-centered": "5.0.11",
    "@storybook/addon-info": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/react": "5.0.10",
    "@types/jest": "24.0.11",
    "@types/lodash.get": "4.4.6",
    "@types/react": "16.8.4",
    "@types/storybook__addon-a11y": "5.0.0",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-centered": "3.3.2",
    "@types/storybook__addon-knobs": "4.0.5",
    "@types/storybook__react": "4.0.1",
    "@types/styled-components": "4.1.14",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.4",
    "concurrently": "^4.1.0",
    "file-loader": "4.0.0",
    "jest": "24.1.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "react-docgen-typescript-loader": "3.0.1",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "ts-jest": "23.10.5",
    "typescript": "3.5.2"
  }
}
